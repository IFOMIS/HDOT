    <!-- http://purl.bioontology.org/ontology/RID/RID1560 -->

    <owl:Class rdf:about="http://purl.bioontology.org/ontology/RID/RID1560">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OGMS_0000090"/>
        <obo:IAO_0000115 xml:lang="en">Operations carried out for the correction of deformities and defects, repair of injuries, and diagnosis and cure of certain diseases.</obo:IAO_0000115>
        <obo:IAO_0000119>http://purl.bioontology.org/ontology/RID/RID1560</obo:IAO_0000119>
        <rdfs:label xml:lang="en">surgical procedure</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.bioontology.org/ontology/RID/RID1649 -->

    <owl:Class rdf:about="http://purl.bioontology.org/ontology/RID/RID1649">
        <rdfs:subClassOf rdf:resource="http://purl.bioontology.org/ontology/RID/RID1560"/>
        <obo:IAO_0000115 xml:lang="en">Transference of a tissue or organ, alive or dead, within an individual, between individuals of the same species, or between individuals of different species. [MeSH]</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">http://purl.bioontology.org/ontology/RID/RID1649</obo:IAO_0000119>
        <rdfs:label xml:lang="en">transplantation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.bioontology.org/ontology/RID/RID39252 -->

    <owl:Class rdf:about="http://purl.bioontology.org/ontology/RID/RID39252">
