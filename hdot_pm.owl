        <obo:IAO_0000115 xml:lang="en">Any molecular entity that contains carbon.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">http://purl.obolibrary.org/obo/CHEBI_50860</obo:IAO_0000119>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000001 -->

    <owl:Class rdf:about="&obo;MFOEM_000001">
        <rdfs:label xml:lang="en">emotion process</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000020"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;RO_0002218"/>
                <owl:someValuesFrom rdf:resource="&obo;OGMS_0000087"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;BFO_0000117"/>
                <owl:someValuesFrom rdf:resource="&obo;MFOEM_000002"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;BFO_0000117"/>
                <owl:someValuesFrom rdf:resource="&obo;MFOEM_000001"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;BFO_0000117"/>
                <owl:someValuesFrom rdf:resource="&obo;MFOEM_000003"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 xml:lang="en">An emotion process is a complex mental process that is a synchronized aggregate of constituent mental processes including an appraisal process as part, and which gives rise to an action tendency.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">http://purl.obolibrary.org/obo/MFOEM_000001</obo:IAO_0000119>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000002 -->

    <owl:Class rdf:about="&obo;MFOEM_000002">
        <rdfs:label xml:lang="en">appraisal process</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000020"/>
        <obo:IAO_0000115 xml:lang="en">An appraisal process is a mental process that gives rise to an appraisal.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">http://purl.obolibrary.org/obo/MFOEM_000002</obo:IAO_0000119>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000003 -->

    <owl:Class rdf:about="&obo;MFOEM_000003">
        <rdfs:label xml:lang="en">physiological response to emotion process</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;GO_0007610"/>
        <obo:IAO_0000115 xml:lang="en">A physiological response to emotion process is a bodily process which encompasses all the neurophysiological changes caused by the appraisal, which take place in the central nervous system (CNS), neuro-endocrine system (NES) and autonomous nervous system (ANS).</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">http://purl.obolibrary.org/obo/MFOEM_000003</obo:IAO_0000119>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000004 -->

    <owl:Class rdf:about="&obo;MFOEM_000004">
        <rdfs:label xml:lang="en">emotional behavioural process</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;GO_0007610"/>
        <obo:IAO_0000115 xml:lang="en">An emotional behavioural process is the behaviour of the organism in response to the appraisal, which includes the characteristic facial expressions for particular emotion types.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">http://purl.obolibrary.org/obo/MFOEM_000004</obo:IAO_0000119>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000005 -->

    <owl:Class rdf:about="&obo;MFOEM_000005">
        <rdfs:label xml:lang="en">appraisal</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000031"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&hdot_core;HDOT_CORE_028"/>
