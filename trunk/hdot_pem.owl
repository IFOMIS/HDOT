        <rdfs:subClassOf rdf:resource="&hdot_core;HDOT_CORE_039"/>
        <obo:IAO_0000115 xml:lang="en">Cognitive ability is an ability which is realized in a cognitive process</obo:IAO_0000115>
    </owl:Class>
    


    <!-- https://hdot.googlecode.com/svn/trunk/hdot_pem.owl#HDOT_PEM_002 -->

    <owl:Class rdf:about="https://hdot.googlecode.com/svn/trunk/hdot_pem.owl#HDOT_PEM_002">
